<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<data>
    <template id="elearning_assets_common" name="Open Mobile Assets" inherit_id="web.assets_common">
        <xpath expr=".">
             <link rel="stylesheet"
              href="/elearning_external_videos/static/src/css/styles.scss"
        />
        </xpath>
    </template>
    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Slides Frontend Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/elearning_external_videos/static/src/js/main_script.js"/>
            <script type="text/javascript" src="/elearning_external_videos/static/src/js/clappr.min.js"/>
        </xpath>
    </template>
    </data>
</odoo>